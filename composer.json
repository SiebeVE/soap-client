{
    "name": "arnedesmedt/soap-client",
    "description": "A general purpose SoapClient library",
    "keywords": [
        "soap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Toon Verwerft",
            "email": "toon.verwerft@phpro.be"
        }
    ],
    "require": {
        "php": "^8",
        "laminas/laminas-code": "~4.0",
        "php-soap/engine": "~1.0",
        "php-soap/ext-soap-engine": "~1.0",
        "php-soap/psr18-transport": "~1.0",
        "psr/event-dispatcher": "~1.0",
        "psr/log": "~1.0",
        "symfony/console": "~4.4 || ~5.0",
        "symfony/event-dispatcher": "~4.4 || ~5.0",
        "symfony/filesystem": "~4.4 || ~5.0",
        "symfony/validator": "~4.4 || ~5.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~7.0",
        "nyholm/psr7": "~1.0",
        "php-http/vcr-plugin": "~1.0",
        "php-parallel-lint/php-parallel-lint": "~1.0",
        "phpro/grumphp-shim": "~1.0",
        "phpspec/phpspec": "dev-main",
        "phpspec/prophecy-phpunit": "~2.0",
        "phpstan/phpstan": "~1.0",
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "Phpro\\SoapClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "PhproTest\\SoapClient\\": "test/"
        }
    },
    "bin": [
        "bin/soap-client"
    ]
}
